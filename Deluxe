    "Silent Aim",
    function(State)
        t_SilentAim = State
    end
)
Combat:Dropdown(
    "Gun Mods",
    {"Remove Firerate", "Inf Damage", "Always Automatic", "No Drop"},
    function(Mod)
        if Mod == "Remove Firerate" then
            Firerate()
        end
        if Mod == "Inf Damage" then
            infD()
        end
        if Mod == "Always Automatic" then
            Auto()
        end
        if Mod == "No Drop" then
            NoDrop()
        end
    end
)
function Firerate()
    for i, v in pairs(getgc(true)) do
        if type(v) == "table" and rawget(v, "firerate") then
            v.firerate = 0.00001
        end
    end
end
function infD()
    for i, v in pairs(getgc(true)) do
        if type(v) == "table" and rawget(v, "firerate") then
            v.damage = 5000
            v.gadgetDamage = 80000
        end
    end
end

function NoDrop()
    for i, v in pairs(getgc(true)) do
        if type(v) == "table" and rawget(v, "firerate") then
            v.velocity = 50000
        end
    end
end


