function CrossHairColor(COLOR)
    for i, v in pairs(game:GetService("Players").Wowtheskyispurple.PlayerGui.Crosshair.Frame:GetChildren()) do
        pcall(
            function()
                v.BackgroundColor3 = COLOR
            end
        )
    end
end
